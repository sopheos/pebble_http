{
    "name": "sopheos/pebble_http",
    "description": "Request / Response library",
    "repositories": [
        {
            "type": "vcs",
            "url": "www@g.sopheos.com:git/pebble_security.git"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "sopheos/pebble_security": "^1.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Pebble\\Http\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
